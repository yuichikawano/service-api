{
  "name": "cuc-pcr-test-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"test/**/*.{ts,js}\"",
    "start": "cross-env NODE_ENV=development NODE_OPTIONS=\"--max-old-space-size=8192\" ts-node-dev -r tsconfig-paths/register --debug --respawn src/main.ts",
    "start:prod": "node -r tsconfig-paths/register dist/src/main",
    "doc": "compodoc",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,js,json}\"",
    "fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:result": "opener ./.pages/cov/index.html",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "yarn typeorm migration:generate -n",
    "typeorm:up": "yarn typeorm migration:run",
    "typeorm:down": "yarn typeorm migration:revert",
    "typeorm:prod": "node $(npm bin)/typeorm -f ormconfig.prod.js",
    "typeorm:prod:create": "yarn typeorm:prod migration:generate -n",
    "typeorm:prod:up": "yarn typeorm:prod migration:run",
    "typeorm:prod:down": "yarn typeorm:prod migration:revert",
    "cmd": "ts-node -r tsconfig-paths/register --project tsconfig.json src/cmd.ts",
    "cmd:prod": "cross-env RUNNING_JS=true node dist/src/cmd.js"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.4.3",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.4.3",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.4.3",
    "@nestjs/swagger": "^4.6.0",
    "@nestjs/typeorm": "^7.1.1",
    "aws-sdk": "^2.741.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.20",
    "mkdirp": "^1.0.4",
    "moment": "^2.27.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.2",
    "typeorm": "^0.2.25",
    "uuid": "^8.3.0",
    "xlsx": "^0.16.6",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.1",
    "@nestjs/testing": "^7.4.3",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.7",
    "@types/faker": "^4.1.12",
    "@types/hapi__joi": "^17.1.4",
    "@types/jest": "26.0.10",
    "@types/lodash": "^4.14.160",
    "@types/mkdirp": "^1.0.1",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.6.1",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.2.13",
    "opener": "^1.5.1",
    "prettier": "^2.1.1",
    "supertest": "^4.0.2",
    "swagger-ui-express": "^4.1.4",
    "ts-jest": "26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.61",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "yarn fix"
    ]
  }
}
